>\n      <v-app-bar\n      app\n      color=\"primary\"\n      dark\n    >\n      <div class=\"d-flex align-center\">\n        <v-toolbar-title>Project PFE</v-toolbar-title>\n      </div>\n\n      <v-spacer></v-spacer>\n\n      \n      <v-btn\n        text\n        v-for=\"item in menuItems\"\n        :key=\"item.title\"\n        :to=\"item.path\">\n        <v-badge\n          v-if=\"item.id == 2 && getJobState != 'finished'\"\n          dot\n          left\n          :bordered=\"false\"\n          color=\"red\"\n          overlap\n        >\n          <v-icon left dark>{{ item.icon }}</v-icon>\n        </v-badge>\n        <v-icon left dark v-else>{{ item.icon }}</v-icon>\n        <span class=\"mr-2\">{{ item.title }}</span>\n      </v-btn>\n      <v-chip\n        class=\"ma-2\"\n        color=\"green\"\n        text-color=\"white\"\n      >\n        <v-icon left dark>mdi-account-tie</v-icon>\n        {{ getUser.username }}\n      </v-chip>\n      <v-btn\n        text\n        @click=\"logout()\">\n        <span class=\"mr-2\">Logout</span>\n        <v-icon left dark>mdi-logout</v-icon>\n      </v-btn>\n      \n    </v-app-bar>\n    <v-overlay :value=\"isLogoutLoading\">\n      <v-progress-circular\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n    </v-overlay>\n    \n</div>\n</template>\n\n<script>\nimport {mapActions,mapGetters} from \"vuex\"\n\nexport default {\n  name: \"NavBar\",\n  data(){\n    return {\n      appTitle: 'PFE Project',\n      sidebar: false,\n      isProcessFinished: false,\n      menuItems: [\n          { title: 'New Crawler', path: '/create_crawler', icon: 'mdi-lead-pencil', id: 1 },\n          { title: 'Crawlers', path: '/crawlers_list', icon: 'mdi-spider', id: 2 },\n     ]\n    }\n  },\n\n  methods: {\n    ...mapActions('Auth',['logout']),\n  },\n  computed: {\n    ...mapGetters(\"Auth\", [\"getUser\"]),\n    ...mapGetters(\"Auth\", [\"isLogoutLoading\"]),\n    ...mapGetters(\"Crawler\",[\"getJobState\"]),\n\n    isCrawlerProcessFinished(){\n      return this.getJobState == 'finished'\n    }\n\n  }\n\n};\n\n</script>\n"],"sourceRoot":"src/components"}]}